inherit-plugin c-clang

function s-clang-vcs()
{
    echo edf
    echo none
    echo git
    echo https://github.com/llvm/llvm-project.git
}

function s-clang-tar()
{
    local VERSION=$1

    echo default
    echo wget
    echo https://minio.retd.edf.fr/salome/YAMM/Public/SALOME/sources/prerequis
    echo llvm-$VERSION.src.tar.gz
}
