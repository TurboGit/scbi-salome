
# Meta-modules with all plug-ins

function s-salome-plugins-modules()
{
    echo s-blsurf-plugin
    echo s-ghs3d-plugin
    echo s-hybrid-plugin
    echo s-gmsh-plugin
    echo s-netgen-plugin
    echo s-hexotic-plugin
    echo s-hexablock-plugin
}

function s-salome-plugins-aggregate()
{
    #  Impossible to use an aggregate project as the aggregated tests
    #  are using common files in the same directory hierarchy.
    echo false
}
