


function gmsh-plugin-build-depends()
{
    echo cmake
    echo sphinx-common
}

function gmsh-plugin-depends()
{
    echo salome-configuration
    echo salome-kernel
    echo salome-gui
    echo omniorb
    echo qt
    echo occt
    echo geom
    echo gmsh
    echo smesh
}

function gmsh-plugin-env()
{
    local PREFIX=$1
    local TARGET=$2

    add-to-var GMSHPLUGIN_ROOT_DIR $PREFIX
}

function gmsh-plugin-config()
{
    local PREFIX=$1
    local TARGET=$2

    cmake -DCMAKE_INSTALL_PREFIX=$PREFIX -DSALOME_GUI_BUILD_WITH_QT5=ON \
          -DCMAKE_BUILD_TYPE=$(get-build-type CMAKE gmsh-plugin Release) \
          ../src
}

function gmsh-plugin-build()
{
    local PREFIX=$1
    local TARGET=$2

    make -j$SCBI_JOBS
}

function gmsh-plugin-install()
{
    local PREFIX=$1
    local TARGET=$2

    make install
}

function gmsh-plugin-prefix()
{
    echo NONE
}
