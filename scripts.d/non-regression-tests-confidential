
function non-regression-tests-confidential-vcs()
{
    echo default
    echo none
    echo git
    echo https://gitlab.pleiade.edf.fr/salome/salome-tests.non_regression_tests_confidential.git
}

function non-regression-tests-confidential-build-depends()
{
    echo cmake
}

function non-regression-tests-confidential-depends()
{
    echo geom
    echo smesh
    echo blsurf-plugin
    echo netgen-plugin
    echo ghs3d-plugin
    echo hexotic-plugin
    echo hybrid-plugin
}

function non-regression-tests-confidential-env()
{
    local PREFIX=$1
    local TARGET=$2

    add-to-var PATH  $PREFIX/bin
}

function non-regression-tests-confidential-config()
{
    local PREFIX=$1
    local TARGET=$2

    cmake -DCMAKE_INSTALL_PREFIX=$PREFIX \
          -DWITH_CAOMAILLAGE_TESTS:BOOL=ON \
          -DWITH_USECASES_TESTS:BOOL=ON \
          -DCMAKE_BUILD_TYPE=$(get-build-type CMAKE non-regression-tests-confidential Release) \
           ../src
}

function non-regression-tests-confidential-build()
{
    local PREFIX=$1
    local TARGET=$2

    make -j$SCBI_JOBS
}

function non-regression-tests-confidential-install()
{
    local PREFIX=$1
    local TARGET=$2

    make install
}

function non-regression-tests-confidential-prefix()
{
    echo NONE
}
