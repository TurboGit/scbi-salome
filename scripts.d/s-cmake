inherit-plugin c-cmake

with-variant native os@-cmake

#  Adds a Windows variant using the MingW64 cmake version
with-variant win \
  --env 'add-to-var PATH $(cygpath -c -s "c:\opt\msys64\mingw64in")'


function s-cmake-build-depends()
{
    echo os@-g++
    echo os@-make
    echo os@-openssl-dev
}
