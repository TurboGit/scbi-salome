

function eficas-module-tar() 
{ 
    local VERSION=$1 
 
    echo default 
    echo none 
    echo none 
    echo eficas-module-$VERSION.tar.xz 
} 

function eficas-module-depends()
{
    echo cmake
    echo salome-configuration
    echo pyqt
    echo salome-kernel
    echo omniorb
    echo salome-gui
}

function eficas-module-config()
{
    local PREFIX=$1
    local TARGET=$2

    cmake -DCMAKE_INSTALL_PREFIX=$PREFIX \
          -DCMAKE_BUILD_TYPE=$(get-build-type CMAKE eficas_module Release) \
          ../src
}

function eficas-module-build()
{
    local PREFIX=$1
    local TARGET=$2

    make -j$SCBI_JOBS
}

function eficas-module-install()
{
    local PREFIX=$1
    local TARGET=$2

    make install
}

function eficas-module-prefix()
{
    echo NONE
}
